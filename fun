window=turtle.Turtle()
window.shape("arrow")
window.shapesize(1)
window.pencolor("red")
window.left(134)
window.forward(110)
window.pencolor("white")
window.forward(145)
window.pencolor("sky blue")
window.color("sky blue")
window.begin_fill()
window.shape("circle")
window.shapesize(5)
window.color("sky blue")
window.end_fill()



cloud=turtle.Turtle()
cloud.shape("arrow")
cloud.shapesize(1)
cloud.pencolor("red")
cloud.left(145)
cloud.forward(135)
cloud.pencolor("white")
cloud.forward(151)
cloud.pencolor("sky blue")
cloud.color("sky blue")
cloud.begin_fill()
cloud.shape("circle")
cloud.shapesize(5)
cloud.color("sky blue")
cloud.end_fill()
turtle.done()

