import turtle
turtle.bgcolor("white")
turtle.pencolor("black")
house=turtle.Turtle()
house.color("black")
house.begin_fill()
house.forward(330)
house.back(430)
house.right(90)
house.forward(279)
house.left(90)
house.forward(430)
house.left(90)
house.forward(279)
house.color("yellow")
house.end_fill()
house.color("black")
house.begin_fill()
house.left(40)
house.forward(100)
house.left(50)
